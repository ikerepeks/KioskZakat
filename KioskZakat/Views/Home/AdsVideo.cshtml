@{
    Layout = null;
}
<!-- Ads Page, can select more ads from Video file -->
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
        #myVideo {
            position: fixed;
            right: 0;
            bottom: 0;
            min-width: 100%;
            min-height: 100%;
        }
    </style>
</head>
<body>

    <a class="navbar-brand align-items-center" asp-area="" asp-controller="Home" asp-action="Index">
        <video autoplay muted loop id="myVideo">
            <source src="~/Video/videoplayback.mp4" type="video/mp4">
            Your browser does not support HTML5 video.
        </video>
    </a>
</body>

